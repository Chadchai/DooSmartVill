<body id="page-top">

  <!-- Page Wrapper -->
  <div id="wrapper">

    <%- include('partials/adminheader.ejs') %>
        <!-- End of Topbar -->

        <!-- Begin Page Content -->
        <div class="container-fluid" style="min-height:75%;">

          <!-- Page Heading -->
          <div class="d-sm-flex align-items-center justify-content-between mb-4">
            <!-- <h1 class="h3 mb-0 text-gray-800" style="color: blue;"><div id="txt"></div></h1> -->
            <!-- <a href="#" class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm"><i class="fas fa-download fa-sm text-white-50"></i> Generate Report</a> -->
          </div>

          <!-- Content Row -->


          <!-- Content Row -->

                     <!-- Color System -->
                     <div class="container" style="background-color: white;width: 100%;">
                      <div class="row justify-content-center">
                          <div class="title col-12 col-lg-12 " style="margin-top: 30px;">
                            <h1 class="m-0 font-weight-bold text-primary" style="text-align: center;"  >
                              ปรับปรุงข้อมูลลูกบ้าน</h1>
                                  <br>
                              
                          </div>
                      </div>
                  </div>
                  <div class="container" style="background-color: white;width: 100%;">
                    <div class="row justify-content-center">
                      <div class="media-container-column col-lg-8" data-form-type="formoid">
                         
          
                          <!---Formbuilder Form--->
                          <form action="/updatehouseinfo" method="POST" class="mbr-form form-with-styler">
                              <div class="row">
                                  
                                  <div hidden="hidden" data-form-alert-danger="" class="alert alert-danger col-12">
                                  </div>
                              </div>
                              
                              
                              <div class="dragArea row">
                                 
                                  <div class="col-md-3  form-group" data-for="email">
                                      <label for="email-form1-h" class="form-control-label mbr-fonts-style display-7">บ้านเลขที่</label>
                                      <input type="tel" name="house_no" data-form-field="Email" required="required" class="form-control display-7 align-left" value="<%= house_info.house_no %>" id="้house_no" readonly>
                                  </div>
                                  <div data-for="phone" class="col-md-2  form-group">
                                      <label for="phone-form1-h" class="form-control-label mbr-fonts-style display-7">ซอยที่</label>
                                      <input type="tel" name="lane_no" data-form-field="Phone" class="form-control display-7" value="<%= house_info.lane_no %>" id="lane_no">
                                  </div>
                                  <div data-for="message" class="col-md-6 form-group">
                                      <label for="message-form1-h" class="form-control-label mbr-fonts-style display-7">ชื่อเจ้าบ้าน</label>
                                      <input type="tel" name="owner_name" data-form-field="Phone" class="form-control display-7" value="<%= house_info.owner_name %>" id="owner_name">
                                  </div>
                        
                                  <div data-for="number" class="col-md-3  form-group">
                                      <label for="phone-form1-h" class="form-control-label mbr-fonts-style display-7">ค่าส่วนกลาง(บาท)</label>
                                      <input type="number" name="common_fee" data-form-field="Phone" class="form-control display-7" value="<%= house_info.common_fee %>" id="common_fee">
                                  </div>
                                  <div data-for="phone" class="col-md-2  form-group">
                                      <label for="phone-form1-h" class="form-control-label mbr-fonts-style display-7">จำนวนรถ</label>
                                      <input type="number" name="parking_qty" data-form-field="Phone" class="form-control display-7 " value="<%= house_info.parking_qty %>" id="parking_qty">
                                  </div>
                                  <div data-for="phone" class="col-md-3  form-group">
                                      <label for="phone-form1-h" class="form-control-label mbr-fonts-style display-7">ค่าจอดรถ/เดือน</label>
                                      <input type="number" name="parking_fee" data-form-field="Phone" class="form-control display-7 " value="<%= house_info.parking_fee %>"  id="parking_fee">
                                  </div>
                                  <div data-for="message" class="col-md-3 form-group">
                                      <label for="message-form1-h" class="form-control-label mbr-fonts-style display-7">หมายเลขล็อค</label>
                                      <input type="tel" name="lock_no" data-form-field="Phone" class="form-control display-7" value="<%= house_info.lock_no %>" id="lock_no">
                                  </div>
                                  <div data-for="message" class="col-md-12 form-group">
                                      <label for="message-form1-h" class="form-control-label mbr-fonts-style display-7">หมายเหตุ</label>
                                      <textarea name="remark" rows="5" data-form-field="Message" class="form-control display-7"  value="<%= house_info.remark %>" id="remark"><%= house_info.remark %></textarea>
                                  </div>
                                  <div class="col-md-12 input-group-btn align-center" style="margin-top: 20px;margin-bottom: 100px;text-align: center;">
                                      <button type="submit" class="btn btn-success btn-form display-4" style="width: 150px;margin: 10px;">บันทึก</button>
                                      <button type="button" class="btn btn-primary btn-form display-4" style="width: 150px;" onclick="window.history.back()">ยกเลิก</button>
                                     
                                  </div>
          
                              </div>
                             
                          </form><!---Formbuilder Form--->
                        
                      </div>
                  </div>
      
        <!-- /.container-fluid -->

      </div>
