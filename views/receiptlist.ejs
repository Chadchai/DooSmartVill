<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

  <title>DooSmartVill - List of Receipt</title>

  <!-- Custom fonts for this template -->
  <link href="assets/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
  <link href="https://fonts.googleapis.com/css?family=Kanit:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

  <!-- Custom styles for this template -->
 <link href="assets/css/sb-admin-2.css" rel="stylesheet">

  <!-- Custom styles for this page -->
  <link href="assets/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">

</head>

<body id="page-top">

  <!-- Page Wrapper -->
  <div id="wrapper">

    <%- include('partials/adminheader.ejs') %>
        <!-- End of Topbar -->

        <!-- Begin Page Content -->
        <div class="container-fluid">

          <!-- Page Heading -->
          
          <!-- <h1 class="h3 mb-2 text-gray-800">ข้อมูลการจอง</h1> -->
          <!-- <p class="mb-4">DataTables is a third party plugin that is used to generate the demo table below. For more information about DataTables, please visit the <a target="_blank" href="https://datatables.net">official DataTables documentation</a>.</p> -->

          <!-- DataTales Example -->
          <div class="card shadow mb-4">
            <div class="card-header py-3">
                <h4 class="m-0 font-weight-bold text-primary">ใบเสร็จรับเงินย้อนหลัง 30 วัน</h4>  
            </div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                  <thead>
                    <tr class="table-heads ">
                        <th class="head-item mbr-fonts-style display-7">ใบเสร็จเลขที่</th>  <th class="head-item mbr-fonts-style display-7" style="width: 13%;">วันที่</th><th class="head-item mbr-fonts-style display-7">บ้านเลขที่</th><th class="head-item mbr-fonts-style display-7">
                            ชื่อเจ้าของบ้าน</th><th class="head-item mbr-fonts-style display-7">จำนวนรวม</th>
                            <th class="head-item mbr-fonts-style display-7">หมายเหตุ</th>
                            <th class="head-item mbr-fonts-style display-7 align-center">พิมพ์ใบเสร็จ</th>
                            <th class="head-item mbr-fonts-style display-7">ยกเลิก</th>
                          </tr>
                  </thead>
                  <tfoot>
                    <tr >
                        <th class="head-item mbr-fonts-style display-7">ใบเสร็จเลขที่</th> <th class="head-item mbr-fonts-style display-7" style="width: 13%;">วันที่</th><th class="head-item mbr-fonts-style display-7">บ้านเลขที่</th><th class="head-item mbr-fonts-style display-7">
                           ชื่อเจ้าของบ้าน</th><th class="head-item mbr-fonts-style display-7">จำนวนรวม</th>
                           <th class="head-item mbr-fonts-style display-7">หมายเหตุ</th>
                           <th class="head-item mbr-fonts-style display-7 align-center">พิมพ์ใบเสร็จ</th>
                           <th class="head-item mbr-fonts-style display-7">ยกเลิก</th>
                         </tr>
                  </tfoot>
                  <% if (receiptlist.length > 0) {%>
               <tbody>
                <% receiptlist.forEach((receipt, index) => { %>
                  <tr> 
                      
                    <td class="body-item mbr-fonts-style display-7"><%= receipt.receipt_no %></td>
                    <td class="body-item mbr-fonts-style display-7"><%= receipt.receipt_date %></td>
                    
                    <td class="body-item mbr-fonts-style display-7"><%= receipt.house_no %></td>
                    <td class="body-item mbr-fonts-style display-7"><%= receipt.owner_name %></td>
                    <td class="body-item mbr-fonts-style display-7 align-center" style="text-align: center;"><%= receipt.t_amount %></td>
                    <td class="body-item mbr-fonts-style display-7"><%= receipt.remark %></td>
                    <td class="body-item mbr-fonts-style display-7 align-center" style="text-align: center;">
                      <a class=".btn-circle"  href="/receiptform1/<%= receipt.house_no %>/<%= receipt.receipt_no %>">พิมพ์</a>
                                   
      
                    </td>
                    <td class="body-item mbr-fonts-style display-7">

                      <% if (receipt.receipt_date == receipt.today) {%>
                      <a class="d-none d-sm-inline-block btn btn-sm btn-danger shadow-sm align-right text-white" onclick="cancelreceipt('<%= receipt.receipt_no %>','<%= receipt.house_no %>','<%= receipt.t_amount1 %>','<%= receipt.id %>','<%= receipt.lastmonth %>','<%= receipt.owner_name %>','<%= receipt.payment_type %>')"><i class="fas fa-edit">ยกเลิก</i></a>
                      <% } %>          
      
                    </td>
                  </tr>
                  <% }) %>
                  </tbody>
                      
                  <% } %>
                </table>
              </div>
            </div>
          </div>

        </div>
        <!-- /.container-fluid -->

      </div>
      <!-- End of Main Content -->

        <!-- Footer -->
        <%- include('partials/adminfooter.ejs') %>
        <!-- End of Footer -->

    </div>
    <!-- End of Content Wrapper -->

  </div>
  <!-- End of Page Wrapper -->

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>

  <!-- Logout Modal-->
  <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
          <button class="close" type="button" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">×</span>
          </button>
        </div>
        <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
        <div class="modal-footer">
          <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
          <a class="btn btn-primary" href="login.html">Logout</a>
        </div>
      </div>
    </div>
  </div>
     <!-- Add New Contact-->
     <div class="modal fade" id="cancelreceipt" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
      <div class="modal-dialog" role="document">
          <form  action="/cancelreceipt" method="post" onsubmit="return " >
        <div class="modal-content">
          <div class="modal-header">
            <h4 class="modal-title text-primary" id="exampleModalLabel">ยกเลิกใบเสร็จนี้</h4>
            <button class="close" type="button" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">×</span>
            </button>
          </div>
          <div class="modal-body">
            
                  <div class="row"> 
                    <div class="col-12"> 
                  <h6 class="modal-title text-primary" >หมายเลขใบเสร็จ: </h6>
                  <input type="text" name="receipt_no" required  id="receipt_no"  class="form-control" readonly> 
             
                
                  </div>
                  <div class="col-12"> 
                      <h6 class="modal-title text-primary" >บ้านเลขที่: </h6>
                      <input type="text" name="homeno" readonly  id="homeno"  class="form-control"> 
                      <input type="hidden" name="receipt_id"  id="receipt_id"  class="form-control" readonly> 
                      </div>
                      <div class="col-12"> 
                        <h6 class="modal-title text-primary" >ชื่อเจ้าของบ้าน: </h6>
                        <input type="text" name="ownername" readonly  id="ownername"  class="form-control"> 
                      
                        </div>
                      <div class="col-12"> 
                        <h6 class="modal-title text-primary" >ยอดในใบเสร็จ </h6>
                        <input type="number" name="amount"  id="amount"  class="form-control" readonly> 
                  
                   
                        </div>
                        <div class="col-12"> 
                          <h6 class="modal-title text-primary" >ประเภทการชำระเงิน </h6>
                          <input type="text" name="payment_type"  id="payment_type"  class="form-control" readonly> 
                    
                     
                          </div>
                        <div class="col-12"> 
                          <h6 class="modal-title text-primary" >ยอดเงินค้างชำระยกมา </h6>
                        
                          <input type="text" name="prev_month"  id="prev_month"  class="form-control" readonly> 
                     
                          </div>
                          <div class="col-12"> 
                            <h6 class="modal-title text-primary" >เหตุผลในการยกเลิก</h6>
                          
                            <input type="text" name="void_reason"  id="void_reason"  class="form-control" required> 
                       
                            </div>
              </div>
             
  
  
          </div>
          <div class="modal-footer">
              <button type="submit" style="width:100px" class="btn btn-success">ยืนยัน</button>
              <button type="button" style="width:100px" class="btn btn-secondary" data-dismiss="modal">ยกเลิก</button>
           
          </div>
        </div>
      </form>
      </div>
    </div>
  <!-- Bootstrap core JavaScript-->
  <script src="assets/vendor/jquery/jquery.min.js"></script>
  <script src="assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

  <!-- Core plugin JavaScript-->
  <script src="assets/vendor/jquery-easing/jquery.easing.min.js"></script>

  <!-- Custom scripts for all pages-->
  <script src="assets/js/sb-admin-2.min.js"></script>

  <!-- Page level plugins -->
  <script src="assets/vendor/datatables/jquery.dataTables.min.js"></script>
  <script src="assets/vendor/datatables/dataTables.bootstrap4.min.js"></script>

  <!-- Page level custom scripts -->
  <script src="assets/js/demo/datatables-demo.js"></script>

  <script>
function cancelreceipt(receiptno,houseno,amount,id,lastmonth,ownername,paymenttype) {
     //console.log("paymenttype" +paymenttype);
     
      document.getElementById("receipt_id").value = id;
document.getElementById("ownername").value = ownername;
document.getElementById("receipt_no").value = receiptno;
document.getElementById("prev_month").value = lastmonth;
document.getElementById("homeno").value = houseno;
document.getElementById("amount").value = amount;
document.getElementById("payment_type").value = paymenttype;

$("#cancelreceipt").modal();
};
  </script>
</body>

</html>
